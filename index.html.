Â  Â  Â  Â  // Generate section analysis
Â  Â  Â  Â  function generateSectionAnalysis(analysis) {
Â  Â  Â  Â  Â  Â  var sectionNames = {
Â  Â  Â  Â  Â  Â  Â  Â  headline: 'Professional Headline',
Â  Â  Â  Â  Â  Â  Â  Â  photo: 'Profile Photo',
Â  Â  Â  Â  Â  Â  Â  Â  about: 'About Section',
Â  Â  Â  Â  Â  Â  Â  Â  experience: 'Experience',
Â  Â  Â  Â  Â  Â  Â  Â  skills: 'Skills & Endorsements',
Â  Â  Â  Â  Â  Â  Â  Â  recommendations: 'Recommendations',
Â  Â  Â  Â  Â  Â  Â  Â  education: 'Education',
Â  Â  Â  Â  Â  Â  Â  Â  featured: 'Featured Content',
Â  Â  Â  Â  Â  Â  Â  Â  licenses: 'Licenses & Certifications',
Â  Â  Â  Â  Â  Â  Â  Â  volunteer: 'Volunteer Experience'
Â  Â  Â  Â  Â  Â  };
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  for (var section in analysis.sectionScores) {
Â  Â  Â  Â  Â  Â  Â  Â  var score = analysis.sectionScores[section];
Â  Â  Â  Â  Â  Â  Â  Â  var status;
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  if (score >= 8) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  status = 'excellent';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  analysis.strengths.push(sectionNames[section] + ' shows strong professional positioning');
Â  Â  Â  Â  Â  Â  Â  Â  } else if (score >= 6) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  status = 'good';
Â  Â  Â  Â  Â  Â  Â  Â  } else if (score >= 4) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  status = 'warning';
Â  Â  Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  status = 'needs-work';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  analysis.issues.push(sectionNames[section] + ' needs improvement');
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  var feedback = getSectionFeedback(section, status);
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  analysis.sections.push({
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  name: sectionNames[section],
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  status: status,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  feedback: feedback,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  score: score
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }

Â  Â  Â  Â  // Get section-specific feedback
Â  Â  Â  Â  function getSectionFeedback(section, status) {
Â  Â  Â  Â  Â  Â  var feedback = {
Â  Â  Â  Â  Â  Â  Â  Â  headline: {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  excellent: 'Headline clearly identifies you as a social work professional',

Â  Â  Â  Â  Â  Â  };
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return feedback[section] ? feedback[section][status] : section + ' section needs attention';
Â  Â  Â  Â  }

Â  Â  Â  Â  // Update score breakdown display
Â  Â  Â  Â  function updateScoreBreakdown(sectionScores) {
Â  Â  Â  Â  Â  Â  var container = document.getElementById('scoreBreakdown');
Â  Â  Â  Â  Â  Â  if (!container) return;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  container.innerHTML = '<h4 style="color: #163C77; margin-bottom: 10px;">Section Breakdown (LinkedIn Weighting)</h4>';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  var sectionNames = {

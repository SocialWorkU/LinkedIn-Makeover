Â  Â  Â  Â  // Analysis function using your weighted system
Â  Â  Â  Â  function performAnalysis(profileText) {
Â  Â  Â  Â  Â  Â  var text = profileText.toLowerCase();
Â  Â  Â  Â  Â  Â  var sectionScores = {};
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Analyze each section
Â  Â  Â  Â  Â  Â  sectionScores.headline = analyzeHeadline(text);
Â  Â  Â  Â  Â  Â  sectionScores.photo = 5; // Base assumption
Â  Â  Â  Â  Â  Â  sectionScores.about = analyzeAbout(text);
Â  Â  Â  Â  Â  Â  sectionScores.experience = analyzeExperience(text);
Â  Â  Â  Â  Â  Â  sectionScores.skills = analyzeSkills(text);
Â  Â  Â  Â  Â  Â  sectionScores.recommendations = 3; // Base assumption
Â  Â  Â  Â  Â  Â  sectionScores.education = analyzeEducation(text);
Â  Â  Â  Â  Â  Â  sectionScores.featured = 2; // Low base
Â  Â  Â  Â  Â  Â  sectionScores.licenses = analyzeLicenses(text);
Â  Â  Â  Â  Â  Â  sectionScores.volunteer = 2; // Low base
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Calculate total weighted score
Â  Â  Â  Â  Â  Â  var totalWeightedScore = 0;
Â  Â  Â  Â  Â  Â  for (var section in PROFILE_WEIGHTS) {
Â  Â  Â  Â  Â  Â  Â  Â  if (sectionScores[section] !== undefined) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  totalWeightedScore += (sectionScores[section] * PROFILE_WEIGHTS[section] * 10);
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  var analysis = {
Â  Â  Â  Â  Â  Â  Â  Â  totalScore: Math.round(totalWeightedScore),
Â  Â  Â  Â  Â  Â  Â  Â  sectionScores: sectionScores,
Â  Â  Â  Â  Â  Â  Â  Â  sections: [],
Â  Â  Â  Â  Â  Â  Â  Â  issues: [],
Â  Â  Â  Â  Â  Â  Â  Â  strengths: [],
Â  Â  Â  Â  Â  Â  Â  Â  text: text
Â  Â  Â  Â  Â  Â  };
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Generate section analysis
Â  Â  Â  Â  Â  Â  generateSectionAnalysis(analysis);
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return analysis;
Â  Â  Â  Â  }

Â  Â  Â  Â  // Individual section analysis functions
Â  Â  Â  Â  function analyzeHeadline(text) {
Â  Â  Â  Â  Â  Â  var score = 0;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Check for social work identifiers
Â  Â  Â  Â  Â  Â  var swIdentifiers = ['social worker', 'lcsw', 'lmsw', 'lgsw', 'lsw'];
Â  Â  Â  Â  Â  Â  for (var i = 0; i < swIdentifiers.length; i++) {
Â  Â  Â  Â  Â  Â  Â  Â  if (text.indexOf(swIdentifiers[i]) !== -1) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  score += 4;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Check for specialization
Â  Â  Â  Â  Â  Â  var specializations = ['clinical', 'school', 'medical', 'healthcare', 'community', 'trauma', 'therapy'];
Â  Â  Â  Â  Â  Â  for (var i = 0; i < specializations.length; i++) {
Â  Â  Â  Â  Â  Â  Â  Â  if (text.indexOf(specializations[i]) !== -1) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  score += 3;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Check for value proposition
Â  Â  Â  Â  Â  Â  var valueWords = ['help', 'support', 'advocate', 'empower', 'specialist'];
Â  Â  Â  Â  Â  Â  for (var i = 0; i < valueWords.length; i++) {
Â  Â  Â  Â  Â  Â  Â  Â  if (text.indexOf(valueWords[i]) !== -1) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  score += 2;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Length check
Â  Â  Â  Â  Â  Â  if (text.length > 50) score += 1;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return Math.min(score, 10);
Â  Â  Â  Â  }


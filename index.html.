Â  Â  Â  Â  // Template copying functionality
Â  Â  Â  Â  function copyToClipboard(button) {
Â  Â  Â  Â  Â  Â  var templateText = button.previousElementSibling.textContent;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  if (navigator.clipboard) {
Â  Â  Â  Â  Â  Â  Â  Â  navigator.clipboard.writeText(templateText.trim()).then(function() {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  var originalText = button.textContent;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.textContent = 'Copied!';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.style.background = '#065f46';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  setTimeout(function() {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.textContent = originalText;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.style.background = '#3CBFBF';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  }, 2000);
Â  Â  Â  Â  Â  Â  Â  Â  }).catch(function(err) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  fallbackCopyTextToClipboard(templateText.trim(), button);
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  fallbackCopyTextToClipboard(templateText.trim(), button);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }

Â  Â  Â  Â  function fallbackCopyTextToClipboard(text, button) {
Â  Â  Â  Â  Â  Â  var textArea = document.createElement("textarea");
Â  Â  Â  Â  Â  Â  textArea.value = text;
Â  Â  Â  Â  Â  Â  document.body.appendChild(textArea);
Â  Â  Â  Â  Â  Â  textArea.focus();
Â  Â  Â  Â  Â  Â  textArea.select();
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  try {
Â  Â  Â  Â  Â  Â  Â  Â  var successful = document.execCommand('copy');
Â  Â  Â  Â  Â  Â  Â  Â  if (successful) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  var originalText = button.textContent;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.textContent = 'Copied!';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.style.background = '#065f46';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  setTimeout(function() {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.textContent = originalText;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  button.style.background = '#3CBFBF';
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  }, 2000);
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  } catch (err) {
Â  Â  Â  Â  Â  Â  Â  Â  showMessage('Copy failed - please copy manually', 'error');
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  document.body.removeChild(textArea);
Â  Â  Â  Â  }
Â  Â  </script>
